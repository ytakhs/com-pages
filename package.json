{
  "name": "remix-template-cloudflare-pages",
  "private": true,
  "sideEffects": false,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"next dev --turbo\" \"tailwindcss --input app/globals.css --output app/output.css --watch\"",
    "build": "tailwindcss app/globals.css --output app/output.css && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{js,ts,tsx,css,json}": "prettier --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "date-fns": "^2.29.2",
    "eslint-config-next": "^13.0.6",
    "marked": "^4.0.19",
    "next": "^13.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@types/marked": "^4.0.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/xml": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2",
    "xml": "^1.0.1"
  },
  "engines": {
    "node": ">=14"
  }
}
